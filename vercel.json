{
  "version": 2,
  "builds": [
    {
      "src": "MyBlog/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1",
      "headers": {
        "Cache-Control": "no-cache", // 临时禁用缓存
        "Content-Type": "text/css"   // 强制MIME类型
      }
    },
    {
      "src": "/(.*)",
      "dest": "MyBlog/wsgi.py"
    }
  ]
}
